package iit_cs586.OutputApi.pumpGasUnit;

import iit_cs586.GasStation;

public class PumpGasUnit1 implements PumpGasUnit {

    @Override
    public void showPumpGasUnit() {
        GasStation.ds1.L = GasStation.ds1.L + 1;
        GasStation.ds1.total = GasStation.ds1.price * GasStation.ds1.L;
    }

}
