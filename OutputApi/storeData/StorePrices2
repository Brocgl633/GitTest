package iit_cs586.strategy.storePrices;

import iit_cs586.GasStation;
import iit_cs586.gaspump.GasPump2;

public class StorePrices2 implements StorePrices {

    @Override
    public void saveData() {
        GasStation.ds2.Rprice = GasStation.gp2_temp_a;
        GasStation.ds2.Pprice = GasStation.gp2_temp_b;
        GasStation.ds2.Dprice = GasStation.gp2_temp_c;
        System.out.println("Gas price updated !!!");
        System.out.println();
    }

}
