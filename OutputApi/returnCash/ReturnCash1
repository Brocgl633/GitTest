package iit_cs586.OutputApi.returnCash;

import iit_cs586.gaspump.GasPump1;

public class ReturnCash1 implements ReturnCash {

    @Override
    public void showReturnCash() {
        int returnCash = GasPump1.ds1.cash - GasPump1.ds1.total;
        System.out.println("Return the remain cash " + returnCash + "$ to you !!!");
        System.out.println();
    }

}
