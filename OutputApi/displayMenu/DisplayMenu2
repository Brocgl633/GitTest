package iit_cs586.OutputApi.displayMenu;

import iit_cs586.GasStation;

public class DisplayMenu2 implements DisplayMenu {

    @Override
    public void showMenu() {
        System.out.println("Please Select Gas Type");
        System.out.println("Regular gas unit price: $" + GasStation.ds2.Rprice);
        System.out.println("Diesel gas unit price: $" + GasStation.ds2.Dprice);
        System.out.println("Premium gas unit price: $" + GasStation.ds2.Pprice);
        System.out.println();
    }

}
