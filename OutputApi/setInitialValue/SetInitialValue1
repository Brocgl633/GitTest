package iit_cs586.OutputApi.setInitialValue;

import iit_cs586.gaspump.GasPump1;

public class SetInitialValue1 implements SetInitialValue {

    @Override
    public void showSetInitialValue() {
        GasPump1.ds1.L = 0;
        GasPump1.ds1.total = 0;
        System.out.println("Gas liter = 0L and total price = 0$ ...");
        System.out.println();
    }

}
