package iit_cs586.OutputApi.setInitialValue;
import iit_cs586.gaspump.GasPump2;

public class SetInitialValue2 implements SetInitialValue {

	@Override
	public void showSetInitialValue() {
		GasPump2.ds2.G = 0;
		GasPump2.ds2.total = 0;
		System.out.println("Gas gallon = 0L and total price = 0$ ...");
		System.out.println();

	}

}
